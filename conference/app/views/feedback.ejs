<!DOCTYPE html>
<html>
    <head>
        <% include partials/template/head.ejs %>
    </head>
    <body id="<% pageID %>">
        <!-- Conference page for Jeonju-North Jeolla KOTESOL -->
        <!-- Inspiration sites:
            Anything by Wes Bos
            https://experiencecode.com
            https://2017.seattle.wordcamp.org/
            http://daremightythings.co/
            https://2017.riga.wordcamp.org/
            http://preview.themeforest.net/item/event-time-conference-event-html-template/full_screen_preview/12858234?ref=blueberry1&clickthrough_id=1110131187&redirect_back=true
            http://demo.ventcampwp.com/?changeset_uuid=6abd48a2-7616-4cda-92cd-06aee541cd23
            http://demo.themeum.com/?theme=eventum
            https://cloudnext.withgoogle.com/
        -->

        <% include partials/template/header-menu.ejs %>

        <main class="site-content">
            <% include partials/content/hero.ejs %>

            <section class="page-section feedback gradient">
                <div class="container">
      <div class="row">
        <div class="col-sm-4">
          <h1 class="feedback-form-title">Send us feedback</h1>
          <form class="feedback-form">
            <div class="form-group">
              <label for="feedback-form-name">Name</label>
              <input type="text" class="form-control" id="feedback-form-name" placeholder="Enter your name">
            </div>

            <div class="form-group">
              <label for="feedback-form-title">Title</label>
              <input type="text" class="form-control" id="feedback-form-title" placeholder="Title of your feedback">
            </div>

            <div class="form-group">
                <label for="feedback-form-message">Message</label>
                <textarea type="text" placeholder="Enter your message, then hit the submit"  class="form-control" id="feedback-form-message" rows="6"></textarea>
            </div>
            <button type="submit" class="btn btn-default pull-right">Submit</button>
          </form>
        </div><!-- primary -->
        <aside class="col-sm-8">
          <div class="maincontent feedback">
              <h2 class="article-title">Recent Feedback</h2>
              <div class="feedback-messages"></div>
          </div>
        </aside><!-- secondary -->
      </div>
    </div><!-- container -->
            </section>
        </main>

        <% include partials/template/footer.ejs %>
        <% include partials/template/footer-scripts.ejs %>
    </body>
</html>
